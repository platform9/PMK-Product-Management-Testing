{
  "apiVersion": "V2",
  "name": "migrationlb",
  "count": 1,
  "haproxy": {
    "frontends": [{
      "bindPort": 8080,
      "protocol": "HTTP",
      "linkBackend": {
        "defaultBackend": "migrationtool"
      }
    }],
    "backends": [{
      "name": "migrationtool",
      "protocol": "HTTP",
      "services": [{
        "marathon": {
          "serviceID": "/migrationtool"
          },
          "endpoint": {
            "portName": "web"
          }
      }]
    }]
  }
}
